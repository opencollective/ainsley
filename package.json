{
  "name": "ainsley",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "yarn build:web && yarn build:repl",
    "build:web": "webpack --config deploy/webpack.web.js --mode production",
    "build:repl": "webpack --config deploy/webpack.repl.js --mode production",
    "repl": "webpack-dev-server --config deploy/webpack.repl.js --mode development",
    "test": "jest",
    "source": "source-map-explorer dist/ainsley.client.js",
    "source:full": "source-map-explorer dist/ainsley.js"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/plugin-transform-typescript": "^7.9.4",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-react": "^7.9.4",
    "@types/codegen.macro": "^3.0.0",
    "@types/csso": "^3.5.1",
    "@types/jest": "^25.1.4",
    "@types/node": "^12.12.31",
    "@types/preval.macro": "^3.0.0",
    "@types/prismjs": "^1.16.0",
    "@types/react": "^16.9.25",
    "@types/react-dom": "^16.9.5",
    "@typescript-eslint/eslint-plugin": "^2.25.0",
    "@typescript-eslint/parser": "^2.25.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-minify-dead-code-elimination": "^0.5.1",
    "core-js": "^3.6.4",
    "css-loader": "^3.4.2",
    "csso": "^4.0.3",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-config-standard": "^14.1.1",
    "eslint-config-standard-with-typescript": "^15.0.1",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^2.5.1",
    "eslint-plugin-standard": "^4.0.1",
    "fast-copy": "^2.0.5",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.2.0",
    "isomorphic-fetch": "^2.2.1",
    "jest": "^25.2.7",
    "json-stringify-pretty-compact": "^2.0.0",
    "prettier": "^2.0.2",
    "prismjs": "^1.20.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-simple-code-editor": "^0.11.0",
    "source-map-explorer": "^2.4.2",
    "source-map-loader": "^0.2.4",
    "standard": "^14.3.3",
    "style-loader": "^1.1.3",
    "ts-loader": "^6.2.2",
    "typescript": "^3.8.3",
    "wasm-brotli": "^2.0.2",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  },
  "dependencies": {
    "ajv": "^6.12.1"
  },
  "jest": {
    "coverageDirectory": "coverage",
    "rootDir": "src",
    "testEnvironment": "node",
    "testMatch": [
      "**/__tests__/**/*.[jt]s?(x)",
      "**/?(*.)+(spec|test).[tj]s?(x)"
    ],
    "transform": {
      "\\.ts$": [
        "babel-jest",
        {
          "configFile": "./deploy/babel.config.js"
        }
      ]
    }
  }
}
